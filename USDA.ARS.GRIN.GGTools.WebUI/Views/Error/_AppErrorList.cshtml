@model USDA.ARS.GRIN.GGTools.ViewModelLayer.SysErrorViewModel
<table id="data-table-sys-app-error" class="table table-striped ggtools-table" style="width:100%;">
    <thead>
        <tr>
            <th></th>
            <th width="5%">ID</th>
            <th>Date</th>
            <th>Message</th>
            <th>Level</th>
            <th>Stack Trace</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var result in Model.DataCollectionSysAppErrors)
        {
            <tr>
                <td>
                    @result.ID
                </td>
                <td>
                    <span class="label bg-aqua">@result.ID</span>
                </td>
                <td>
                    @result.CreateDate.ToString("yyyy'-'MM'-'dd'T'HH':'mm':'ss")
                </td>
                <td>
                    @result.ErrorMessage
                </td>
                <td>
                    @result.ErrorLevel
                </td>
                <td>
                    @result.StackTrace
                </td>
            </tr>
        }
    </tbody>
</table>






