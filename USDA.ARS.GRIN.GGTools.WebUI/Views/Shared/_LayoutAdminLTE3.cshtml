            </div>
                @switch (Session["APP_CONTEXT"])
            {
                case "GGT-TAX":
                    @Html.Partial("~/Views/Shared/Sidebars/_MainSidebarTaxonomy.cshtml")
                    break;
                case "GGT-ORD":
                    @Html.Partial("~/Views/Shared/Sidebars/_MainSidebar.cshtml")
                    break;
                case "GGT-NRR":
                    @Html.Partial("~/Views/Shared/Sidebars/_MainSidebarNRR.cshtml")
                    break;
                case "GGT-CUR":
                    @Html.Partial("~/Views/Shared/Sidebars/_MainSidebar.cshtml")
                    break;
                case "GGT-ARM":
                    @Html.Partial("~/Views/Shared/Sidebars/_MainSidebarARM.cshtml")
                    break;
                default:
                    @Html.Partial("~/Views/Shared/Sidebars/_MainSidebar.cshtml")
                    break;
            }
