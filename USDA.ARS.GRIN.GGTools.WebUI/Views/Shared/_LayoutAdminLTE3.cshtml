        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <a href="@Url.Action("Index","Home")" class="brand-link bg-s bg-success">
                <img src="~/dist/img/GGlogo.gif" alt="GRIN-Global Logo" class="brand-image img-circle elevation-3" style="opacity: .8">
                <span class="brand-text font-weight-light">
                    @if (Session["APP_CONTEXT"].ToString().Trim() == "GGT-TAX")
                    {
                        <span>TurboTaxon</span>
                    }
                    else
                    {
                        <span>GGTools</span>
                    }
                </span>
            </a>
