@model USDA.ARS.GRIN.GGTools.ViewModelLayer.FolderViewModel
<div class="alert alert-success alert-dismissible">
    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
    <h5><i class="icon fas fa-check"></i>Folder Created Successfully</h5>
    The folder <strong>@Model.Entity.FolderName</strong> has been created.&nbsp;
    <a href="@Url.Action("Edit","Folder", new { entityId = Model.Entity.ID })">View Folder</a>
</div>
