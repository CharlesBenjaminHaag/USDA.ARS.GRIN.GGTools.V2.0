@model USDA.ARS.GRIN.GGTools.ViewModelLayer.WebOrderRequestViewModel
<input type="hidden" id="hf_current_filter_most_recent_action" />
<div class="card card-default">
    <div class="card-header">
        <h3 class="card-title">
            <i class="fas fa-filter"></i>
            Most Recent Action
        </h3>
    </div>
    <!-- /.card-header -->
    <div class="card-body">
        <div id="section-most-recent-action">
            @foreach (var result in Model.WebOrderRequestActions)
            {
                <label class="button-checkbox">
                    <input id="filter-most-recent-action-@result.Value" type="checkbox" data-ggtools-action="@result.Value" />
                    <span>@result.Value</span>
                </label>
            }
        </div>
    </div>
</div>
