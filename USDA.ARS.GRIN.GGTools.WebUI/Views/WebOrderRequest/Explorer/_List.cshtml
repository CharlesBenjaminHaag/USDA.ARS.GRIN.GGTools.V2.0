@model USDA.ARS.GRIN.GGTools.ViewModelLayer.WebOrderRequestViewModel
<table id="data-table-web-order-requests" class="table table-striped ggtools" style="width:100%;">
    <thead>
        <tr>
            <th style="width: 1%">
                ID
            </th>
            
            <th style="width: 20%">
                Requestor
            </th>
            <th>Submitted</th>
            <th style="width: 8%" class="text-center">
                Intended Use
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var result in Model.DataCollection)
        {
            <tr>
                <td>
                    @result.ID
                </td>
                
                <td>
                    <a>
                      <strong>@result.WebCooperatorFullName</strong>
                    </a>
                    <br>
                    <small>
                        @result.WebCooperatorEmail
                    </small>
                    <br />
                    <small>
                        @result.WebCooperatorOrganization
                    </small>
                </td>
                <td>@result.CreatedDate.ToShortDateString()</td>
                <td class="project-state">
                    @result.IntendedUseNote
                </td>
            </tr>
        }
    </tbody>
</table>
<script type="text/javascript">
    $(document).ready(function () {
        InitWebOrderRequestDataTable("data-table-web-order-requests");
    });
</script>
