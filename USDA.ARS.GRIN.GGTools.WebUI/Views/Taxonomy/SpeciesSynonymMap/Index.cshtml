@model USDA.ARS.GRIN.GGTools.Taxonomy.ViewModelLayer.SynonymMapViewModel
@{
    ViewBag.Title = Model.PageTitle;
    Layout = "~/Views/Shared/_LayoutAdminLTE3.cshtml";
}

@using (Html.BeginForm("Search", "SpeciesSynonymMap", FormMethod.Post, new { id = "frmMain" }))
{
    @Html.HiddenFor(x => x.EventAction)
    @Html.HiddenFor(x => x.TableName)
    @Html.HiddenFor(x => x.TableCode)
    @Html.HiddenFor(x => x.PageTitle)
    @Html.HiddenFor(x => x.AuthenticatedUserCooperatorID)
    @Html.HiddenFor(x => x.ItemIDList)
    @Html.HiddenFor(x => x.AuthenticatedUserCooperatorID)
    @Html.Partial("~/Views/Taxonomy/SpeciesSynonymMap/_Detail.cshtml")
}
@Html.Partial("~/Views/Taxonomy/SpeciesSynonymMap/_List.cshtml")
@{Html.RenderAction("_Edit", "Folder", new { @tableName = Model.TableName }); }

<script type="text/javascript">
    $(document).ready(function () {
        Init();
        SetExtendedFields();
    });

    $(document).keypress(function (event) {
        var keycode = (event.keyCode ? event.keyCode : event.which);
        if (keycode == '13') {
            $("#btnSearch").click();
        }
    });
</script>