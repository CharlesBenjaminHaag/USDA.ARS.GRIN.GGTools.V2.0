@model USDA.ARS.GRIN.GGTools.Taxonomy.ViewModelLayer.FamilyMapViewModel
<table id="data-table-family-lookup" class="table table-striped" style="width:100%;">
    <thead>
        <tr>
            <th></th>
            <th></th>
            <th>ID</th>
            <th>Name</th>
            <th>Accepted</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var result in Model.DataCollection)
        {
        <tr>
            <td>@result.ID</td>
            <td>@result.FamilyName</td>
            <td>@result.ID</td>
            <td>@Html.Raw(result.AssembledName)</td>
            <td>@Html.Raw(Model.FormatBoolean(result.IsAcceptedName))</td>
        </tr>
        }
    </tbody>
</table>
<script type="text/javascript">
    $(document).ready(function () {
        InitDataTableWithAssembledName("data-table-family-lookup");
    });
</script>